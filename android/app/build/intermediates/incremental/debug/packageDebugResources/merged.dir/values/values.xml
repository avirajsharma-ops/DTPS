<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#E06A26</color>
    <color name="colorPrimary">#10B981</color>
    <color name="colorPrimaryDark">#059669</color>
    <color name="ic_launcher_background">#FFFFFF</color>
    <color name="splash_background">#FFFFFF</color>
    <string name="app_name">DTPS</string>
    <string name="custom_url_scheme">com.dtps.app</string>
    <string name="package_name">com.dtps.app</string>
    <string name="title_activity_main">DTPS</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:background">@null</item>
        <item name="android:statusBarColor">@color/colorPrimary</item>
    </style>
    <style name="AppTheme.NoActionBarLaunch" parent="Theme.SplashScreen">
        <item name="android:background">@drawable/splash</item>
    </style>
</resources>